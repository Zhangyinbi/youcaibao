                        } else if (!etPayPwd.getText().toString().equals(etPayPwd2.getText().toString())) {
                            ToastUtil.show(mContext, "支付密码两次输入不相同");
                        } else {
                            setPaypwd(etPassword.getText().toString(), etPayPwd.getText().toString(),
                                    etPayPwd2.getText().toString());
                        }
                    }
                });
            }
        });
    }


